{
  "version": 2,
  "builds": [
    {
      "src": "dist/app.js",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "pnpm run build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
